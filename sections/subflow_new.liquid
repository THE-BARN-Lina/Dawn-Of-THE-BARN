{% comment %}ly_global_begin{% endcomment %}
{%- capture ly-global-content %}{% render 'ly-global' render:true %}{% endcapture -%}
{%- assign ly-global-content = ly-global-content | split: '#ly#' -%}
{%- assign langify_title = ly-global-content[0] -%}
{%- assign langify_description = ly-global-content[1] -%}
{%- assign langify_shop_name = ly-global-content[2] -%}
{%- assign langify_shop_description = ly-global-content[3] -%}
{%- assign canonical_langify_url = ly-global-content[4] -%}
{%- assign langify_shop_url = ly-global-content[5] -%}
{%- assign langify_shop_domain = ly-global-content[6] -%}
{% comment %}ly_global_end{% endcomment %}
<div class="prod_select_outer page-width">
  <h2 class="prod_select_hed">{{ section.settings.subscription_heading }}</h2>
  <ul class="prod_select_inner grid grid--uniform grid--view-items">
    {%- for block in section.blocks -%}
      {%- if block.type == 'icon_text' -%}
        <li class="prod_select_content grid__item small--one-half medium-up--one-quarter text-center">
          <div class="hoverColor">
            <a
              href="javascript:;"
              data-val="{{block.settings.title_value | downcase | handle }}"
              class="hoverColor prod_select_content_inner btn_select_prod"
            >
              <h2 class="prod_select_text" data-val="{{ block.settings.title_value | handle }}">
                {{ block.settings.title }}
              </h2>
              <p class="prod_select_cont">{{ block.settings.select_sub_hed }}</p>
              <div class="prod_img_outer">
                <img class="prod_img" src="{{block.settings.select_img | img_url:'master'}}">
              </div>
            </a>
          </div>
          <div class="btn_outer_select">
            <a
              class="btn btn_select_prod"
              href="javascript:;"
              data-val="{{block.settings.title_value | downcase | handle }}"
            >
              {% comment %}ly_i18n_replace_for_[ 'general.subscription_funnel.select_label' | t ]_begin{% endcomment %}
              {%- capture ly_template %}{{ 'general.subscription_funnel.select_label' | t }}{% endcapture -%}
              {%- capture ly_translation %}{% render 'ly-static-string' with '19648886' render:true %}{% endcapture -%}
              {%- if ly_translation != '' and ly_translation != null -%}
                {%- assign escape_content = true -%}
                {%- assign ly_template = ly_translation -%}
              {%- else -%}
                {%- assign escape_content = false -%}
              {%- endif -%}
              {%- if escape_content %}{{ ly_template | escape }}{% else %}{{ ly_template }}{% endif -%}
              {% comment %}ly_i18n_replace_end{% endcomment %}
            </a>
          </div>
        </li>
      {%- endif -%}
    {%- endfor -%}
  </ul>
</div>

<style>
  .hoverColor a:hover,
  .hoverColor a:hover p
  {

     color: #68431B;
  }

  .specialOffersTitle
  {
    font-size: 26px;
    margin: 25px 0;
    line-height: normal;
    letter-spacing: 1px;
    text-align: center;
    margin-top: 100px;
  }

  .customHR
  {
    width: 80vw;
    margin-left: auto;
    margin-right: auto;
    margin-bottom: 100px;
  }
  .que_select.que_select_4.opts_ans {
    display: flex;
    align-items: flex-start;
    overflow-x: auto;
  }
  .que_select.que_select_4.opts_ans .opt_answ {
    flex: 1;
    width: 100%;
    max-width: 120px;
  }
  .sub_flow_new .prod_selector_outer .prod_selector_sec .que_select_4 label {
    width: 100%;
  }
</style>

<div class="prod_selector_outer page-width">
  {% for block in section.blocks %}
    {% if block.type == 'icon_text' %}
      <div class="prod_selector_inner" data-val="{{ block.settings.title_value | handle }}">
        <h2 class="prod_sec_text">{{ block.settings.title }}</h2>
        {{ block.settings.sec_subhed }}
      </div>
    {% endif %}
  {% endfor %}

  <div class="prod_selector_sec subWidget">
    {%- for block in section.blocks -%}
      {%- if block.type == 'icon_text' -%}
        <div class="prod_select_left type-wrapper-js" data-val="{{ block.settings.title_value | handle }}">
          {% comment %}Question 1{% endcomment %}
          {%- if block.settings.que1_type1 != blank -%}
            <div class="question que1">
              <div class="que_hed">
                <h3>{{ block.settings.question_heading }}</h3>
              </div>
              {% assign qone_val = block.settings.que1_type1_val | downcase | handle %}
              {% assign qtwo_val = block.settings.que1_type2_val | downcase | handle %}
              {% assign qone_label = block.settings.que1_type1 %}
              {% assign qtwo_label = block.settings.que1_type2 %}
              <div class="que_select que_select_1 opts_ans">
                <div class="opt_answ">
                  <input
                    class="check_red"
                    type="radio"
                    name="style"
                    id="{{ block.settings.title_value | handle }}-{{qone_val}}"
                    data-label="{{ qone_label }}"
                    value="{{qone_val}}"
                  >
                  <label for="{{ block.settings.title_value | handle }}-{{qone_val}}">
                    <div>{{ qone_label }}</div>
                  </label>
                </div>
                {%- if qtwo_label != blank -%}
                  <div class="opt_answ">
                    <input
                      class="check_red"
                      type="radio"
                      name="style"
                      id="{{ block.settings.title_value | handle }}-{{qtwo_val}}"
                      data-label="{{ qtwo_label }}"
                      value="{{qtwo_val}}"
                    >
                    <label for="{{ block.settings.title_value | handle }}-{{qtwo_val}}">
                      <div>{{ qtwo_label }}</div>
                    </label>
                  </div>
                {%- endif -%}
              </div>
            </div>
          {%- endif -%}

          {% comment %}Question 2{% endcomment %}
          {%- if block.settings.que2_type1 != blank -%}
            <div class="question que2 for_grm">
              <div class="que_hed">
                <h3>{{ block.settings.question2_heading }}</h3>
                <h3>{{ block.settings.question2_heading_2 }}</h3>
              </div>
              {% assign q2_val1 = block.settings.que2_type1 | downcase | handle %}
              {% assign q2_val2 = block.settings.que2_type2 | downcase | handle %}
              {% assign q2_val3 = block.settings.que2_type3 | downcase | handle %}
              <div class="que_select que_select_2 opts_ans">
                <div class="opt_answ">
                  <input
                    class="check_red"
                    type="radio"
                    name="weight"
                    id="{{ block.settings.title_value | handle }}-{{q2_val1}}"
                    value="{{q2_val1}}x250"
                  >
                  <label for="{{ block.settings.title_value | handle }}-{{q2_val1}}">
                    <div>{{ block.settings.que2_type1 }}</div>
                  </label>
                </div>

                <div class="opt_answ">
                  <input
                    class="check_red"
                    type="radio"
                    name="weight"
                    id="{{ block.settings.title_value | handle }}-{{q2_val2}}"
                    value="{{q2_val2}}x250"
                  >
                  <label for="{{ block.settings.title_value | handle }}-{{q2_val2}}">
                    <div>{{ block.settings.que2_type2 }}</div>
                  </label>
                </div>

                <div class="opt_answ">
                  <input
                    class="check_red"
                    type="radio"
                    name="weight"
                    id="{{ block.settings.title_value | handle }}-{{q2_val3}}"
                    value="{{q2_val3}}x250"
                  >
                  <label for="{{ block.settings.title_value | handle }}-{{q2_val3}}">
                    <div>{{ block.settings.que2_type3 }}</div>
                  </label>
                </div>
              </div>
            </div>
          {%- endif -%}

          {%- if block.settings.que2_type1_1kg != blank -%}
            <div class="question que2 for_kg">
              <div class="que_hed">
                <h3>{{ block.settings.question2_heading_1kg }}</h3>
                <h3>{{ block.settings.question2_heading_2_1kg }}</h3>
              </div>
              {% assign q2_val1 = block.settings.que2_type1_1kg | downcase | handle %}
              {% assign q2_val2 = block.settings.que2_type2_1kg | downcase | handle %}
              {% assign q2_val3 = block.settings.que2_type3_1kg | downcase | handle %}
              <div class="que_select que_select_2 opts_ans">
                <div class="opt_answ">
                  <input class="check_red" type="radio" name="weight" id="k{{q2_val1}}" value="{{q2_val1}}x 1">
                  <label for="k{{q2_val1}}">
                    <div>{{ block.settings.que2_type1_1kg }}</div>
                  </label>
                </div>

                <div class="opt_answ">
                  <input class="check_red" type="radio" name="weight" id="k{{q2_val2}}" value="{{q2_val2}}x 1">
                  <label for="k{{q2_val2}}">
                    <div>{{ block.settings.que2_type2_1kg }}</div>
                  </label>
                </div>

                <div class="opt_answ">
                  <input class="check_red" type="radio" name="weight" id="k{{q2_val3}}" value="{{q2_val3}}x 1">
                  <label for="k{{q2_val3}}">
                    <div>{{ block.settings.que2_type3_1kg }}</div>
                  </label>
                </div>
              </div>
            </div>
          {%- endif -%}

          {% comment %}Question 4{% endcomment %}
          {%- if block.settings.que4_type1 != blank -%}
            <div class="question que4">
              <div class="que_hed">
                <h3>{{ block.settings.question4_heading }}</h3>
              </div>
              {% assign q4_val1 = block.settings.que4_type1_val | downcase %}
              {% assign q4_val2 = block.settings.que4_type2_val | downcase %}
              {% assign q4_val3 = block.settings.que4_type3_val | downcase %}
              {% assign q4_val4 = block.settings.que4_type4_val | downcase %}
              {% assign q4_label1 = block.settings.que4_type1 %}
              {% assign q4_label2 = block.settings.que4_type2 %}
              {% assign q4_label3 = block.settings.que4_type3 %}
              {% assign q4_label4 = block.settings.que4_type4 %}
              <div class="que_select que_select_4 opts_ans">
                {%- if block.settings.que4_img1 != blank -%}
                  <div class="opt_answ {{q4_val1}}">
                    <input
                      class="check_red"
                      type="radio"
                      name="destination"
                      id="{{ block.settings.title_value | handle }}-{{q4_val1}}"
                      {% if block.settings.que4_deafault_weight != blank %}
                        data-default-weight="{{block.settings.que4_deafault_weight}}"
                      {% endif %}
                      data-label="{{ q4_label1 }}"
                      value="{{q4_val1}}"
                    >
                    <label for="{{ block.settings.title_value | handle }}-{{q4_val1}}">
                      <div>
                        {{ q4_label1 }}
                        <img src="{{ block.settings.que4_img1 | img_url:'master' }}">
                      </div>
                    </label>
                  </div>
                {%- endif -%}

                {%- if block.settings.que4_img2 != blank -%}
                  <div class="opt_answ {{q4_val2}}">
                    <input
                      class="check_red"
                      type="radio"
                      name="destination"
                      {% if block.settings.que4_deafault_weight != blank %}
                        data-default-weight="{{block.settings.que4_deafault_weight}}"
                      {% endif %}
                      id="{{ block.settings.title_value | handle }}-{{q4_val2}}"
                      data-label="{{ q4_label2 }}"
                      value="{{q4_val2}}"
                    >
                    <label for="{{ block.settings.title_value | handle }}-{{q4_val2}}">
                      <div>
                        {{ q4_label2 }}
                        <img src="{{ block.settings.que4_img2 | img_url:'master' }}">
                      </div>
                    </label>
                  </div>
                {%- endif -%}

                {%- if block.settings.que4_img3 != blank -%}
                  <div class="opt_answ {{q4_val3}}">
                    <input
                      class="check_red"
                      type="radio"
                      name="destination"
                      {% if block.settings.que4_deafault_weight != blank %}
                        data-default-weight="{{block.settings.que4_deafault_weight}}"
                      {% endif %}
                      id="{{ block.settings.title_value | handle }}-{{q4_val3}}"
                      data-label="{{ q4_label3 }}"
                      value="{{q4_val3 }}"
                    >
                    <label for="{{ block.settings.title_value | handle }}-{{q4_val3}}">
                      <div>
                        {{ q4_label3 }}
                        <img src="{{ block.settings.que4_img3 | img_url:'master' }}">
                      </div>
                    </label>
                  </div>
                {%- endif -%}

                {%- if block.settings.que4_img4 != blank -%}
                  <div class="opt_answ {{q4_val4}}">
                    <input
                      class="check_red"
                      type="radio"
                      name="destination"
                      {% if block.settings.que4_deafault_weight != blank %}
                        data-default-weight="{{block.settings.que4_deafault_weight}}"
                      {% endif %}
                      id="{{ block.settings.title_value | handle }}-{{q4_val4}}"
                      data-label="{{ q4_label4 }}"
                      value="{{q4_val4 }}"
                    >
                    <label for="{{ block.settings.title_value | handle }}-{{q4_val4}}">
                      <div>
                        {{ q4_label4 }}
                        <img src="{{ block.settings.que4_img4 | img_url:'master' }}">
                      </div>
                    </label>
                  </div>
                {%- endif -%}
              </div>
            </div>
          {%- endif -%}

          {% comment %}Question 5{% endcomment %}
          {%- if block.settings.que5_type1 != blank -%}
            <div class="question que5">
              <div class="que_hed">
                <h3>{{ block.settings.question5_heading }}</h3>
              </div>
              {% assign q5_val1 = block.settings.que5_type1 | downcase | handle %}
              {% assign q5_val2 = block.settings.que5_type2 | downcase | handle %}

              <div class="que_select que_select_5 opts_ans">
                <div class="opt_answ">
                  <input class="check_red" type="radio" name="period" id="nongoing" value="ongoing">
                  <label for="nongoing">
                    <div>{{ block.settings.que5_type1 }}</div>
                  </label>
                </div>
                <div class="opt_answ">
                  <input
                    class="check_red"
                    type="radio"
                    name="period"
                    id="{{ block.settings.title_value | handle }}-{{q5_val2}}"
                    value="3"
                  >
                  <label for="{{ block.settings.title_value | handle }}-{{q5_val2}}">
                    <div>{{ block.settings.que5_type2 }}</div>
                  </label>
                </div>
                <div class="plan_show hide">
                  <select id="month" class="plan_counts">
                    <option value="3">3</option>
                    <option value="6">6</option>
                    <option value="12">12</option>
                  </select>
                  <label for="month" class="plan_hed"><ly-as-19648882>Months</ly-as-19648882></label>
                </div>
              </div>
            </div>
          {%- endif -%}
        </div>
      {%- endif -%}
    {%- endfor -%}

    <div class="prod_select_right summary">
      <h2 class="summary_had"><ly-as-19648883>SUMMARY</ly-as-19648883></h2>
      <div class="summary_list_outer">
        <div class="summary_answ summary_answ_1">
          <div class="select_val_1">
            <img class="summary-icon" id="dmf" src="{{section.settings.summery_style_img | img_url:'master'}}">
            <span class="select_rost" id="select_rost"></span>
            {% comment %}ly_i18n_replace_for_[ 'general.subscription_funnel.roast' | t ]_begin{% endcomment %}
            {%- capture ly_template %}{{ 'general.subscription_funnel.roast' | t }}{% endcapture -%}
            {%- capture ly_translation %}{% render 'ly-static-string' with '19648887' render:true %}{% endcapture -%}
            {%- if ly_translation != '' and ly_translation != null -%}
              {%- assign escape_content = true -%}
              {%- assign ly_template = ly_translation -%}
            {%- else -%}
              {%- assign escape_content = false -%}
            {%- endif -%}
            {%- if escape_content %}{{ ly_template | escape }}{% else %}{{ ly_template }}{% endif -%}
            {% comment %}ly_i18n_replace_end{% endcomment %}
          </div>
        </div>
        <div class="summary_answ summary_answ_2">
          <div class="select_val_2">
            <img class="summary-icon" id="bagno" src="{{section.settings.summery_weight_img | img_url:'master'}}">
            <span class="select_bags" id="select_bags"></span>
          </div>
        </div>
        <div class="summary_answ summary_answ_3">
          <div class="select_val_4">
            <img class="summary-icon" id="flagno" src="{{section.settings.summery_destination_img | img_url:'master'}}">
            <span class="select_location" id="select_location"></span>
          </div>
        </div>
      </div>
      <div class="summary_moreinfo">
        <div class="info_cont">
          {{ section.settings.summery_info }}
        </div>
        <div class="info_cont info_cont_special_summery hide">
          <p>
            <ly-as-19648884
              >“Your first delivery will be a sample box. It will automatically turn into a curated service after a
              month"</ly-as-19648884
            >
          </p>
        </div>
      </div>
      <div class="cartbox">
        <div class="summary-price"><ly-as-19648885>€000.00</ly-as-19648885></div>
        <input type="submit" value="Checkout" name="subscribe" class="summary-checkout btn" style="">
      </div>
    </div>
  </div>
</div>

