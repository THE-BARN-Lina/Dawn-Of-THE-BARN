<h2>{{ section.settings.h2_Heading }}</h2>

<style>
  h2 {
    text-align:center;
  }
</style>

{% schema %}
{
  "name": "h2_Heading",
  "settings": [
                  {
                    "type":"text",
                    "label":"h2 Heading",
                    "id":"h2_Heading",
                    "default":"Insert text here"
                  },
     {
      "type": "range",
      "id": "padding_top",
      "min": 0,
      "max": 100,
      "step": 4,
      "unit": "px",
      "label": "t:sections.all.padding.padding_top",
      "default": 40
    },
    {
      "type": "range",
      "id": "padding_bottom",
      "min": 0,
      "max": 100,
      "step": 4,
      "unit": "px",
      "label": "t:sections.all.padding.padding_bottom",
      "default": 52
    }
              ],
  "presets":[
      {
        "name":"h2 Heading"
      }
    ]
}
{% endschema %}
