{%- style -%}
:root {
  --brown: #804d15;
  --white: #ffffff;
  --black:#282828;
  --padding-sections:#282828;
  --text-shadow:#bcbcbc;
  --button-main:black;
  --font-main: "Chalet Comprime";
  --font-secondary:"Raleway";
  --font-size-big:6vh;
  --font-size-medium:4vh;
  --font-size-medium-small:3.5vh;
  --font-size-small:14px;
}


h1{
  font-family: var(--font-main);
  font-size: var(--font-size-big);
  margin:0px;
  padding:0px;
  line-height: normal;
}
h2{
  font-family: var(--font-main);
  font-size: var(--font-size-medium);
  margin:0px;
  padding:0px;
  line-height: normal;
}
h3{
  font-family: var(--font-main);
  font-size: var(--font-size-medium-small);
  margin:0px;
  padding:0px;
  line-height: normal;
}
h4{
  margin:0px;
  padding:0px;
  line-height: normal;
}
p{
  font-family:var(--font-secondary);
  font-size:var(--font-size-small);
  margin:0px;
  padding:0px;
  line-height: normal;
}
.button-main{
  background-color:var(--brown);
  padding:1.6rem;
  border-radius:5px;
  color: var(--white);
  min-width:20rem; 
  font-size: 16px;
  border:none;  
}

{% comment %} ///////////////////HERO SECTION///////////////////// {% endcomment %}
.master-section-container-hero{
  height:calc(100vh - 168.688px);
  width:100vw;
  border:20px solid var(--padding-sections);
  display:flex;
  flex-direction:column;
  align-items:center;
  text-align:center;
  justify-content:center;
}
{% comment %} ///////////////////SECOND SECTION///////////////////// {% endcomment %}
.master-section-container{
  height:100vh;
  width:100vw;
  border:20px solid var(--padding-sections);
}
.master-title-subtitle-button-container{
  height:20vh;
  display:flex;
  flex-direction:row;
  justify-content:space-between;
  align-items:center;
  padding:20px;

}
.master-title-subtitle-container{
  width:50%;
}
.master-content-container{


  overflow-x: auto;
  -webkit-overflow-scrolling: touch; /* Enables momentum-based scrolling on iOS devices */
  scroll-snap-type: x mandatory; 


  display:flex;
  flex-direction:row;
  justify-content:space-between;
  align-items:center;

  background-color:var(--padding-sections);
  width:100%;
  height: calc(100vh - 20vh - 40px );
}
.content-box-triple{
  height:100%;
  max-width:33.33%;
  min-width:400px;
  border:2px solid var(--padding-sections);

  padding:0px;


  display:flex;
  flex-direction:column;
  justify-content:space-between;
  text-align:center;
  align-items:center;
  scroll-snap-align: start; 
}
.master-image{
  width:100%;
  height:60%;
  object-fit:cover;
}
.text-content-container{
  height:40%;
  background-color:black;
  padding:20px;

  display:flex;
  flex-direction:column;
  justify-content:center;
  text-align:center;
  align-items:center;
}
.arrow-right-container{
  background-color:black;
  width:100%;

  display:flex;
  flex-direction:row;
  justify-content:flex-end;
  align-items:center;

  padding-right:1rem; 
  padding-bottom:1rem;


  
}


@media (max-width: 1100px) {
  .master-content-container{
    background-color:var(--padding-sections);
    height: calc(100vh - 30vh - 40px );

  }
  .master-title-subtitle-button-container{
    height:20vh;
    display:flex;
    flex-direction:row;
    justify-content:center;
    align-items:center;
    padding:20shpx;
    background-color:black;

  }
  .master-image{
    height:50%; 
  }
  .text-content-container{
    display: flex;
    flex-direction: column;
    align-items: flex-start;
    justify-content:start;
    text-align:start;
    height:40%;


  }
  .content-box-triple{
    height:100%;
    min-width:calc(100vw - 60px);
    padding:0px;
    justify-content:flex-start;

  }
  .button-desktop{
    display:none;
  }
  .master-title-subtitle-container{
    text-align:center;
    width:100%;
  }
  .master-container-button-mobile{
    display:flex;
    flex-direction:column;
    justify-content:center;
    text-align:center;
    align-items:center;
    height:10%;

    background-color:black;

  }
}
@media (min-width: 1100px) {

  .button-mobile{
    display:none;
  }
  .arrow-right-container{
    display:none;
  }

}
