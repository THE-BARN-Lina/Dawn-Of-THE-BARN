document.addEventListener('DOMContentLoaded', function () {
    var accButtons = document.querySelectorAll('.accordion-button');
    accButtons.forEach(function(btn) {
        btn.addEventListener('click', function() {
            var panel = this.nextElementSibling;
            var arrow = this.querySelector('.arrow');
            if (panel.style.maxHeight) {
                panel.style.maxHeight = null;
                arrow.innerHTML = '&#9660;';
            } else {
                document.querySelectorAll('.panel').forEach(p => p.style.maxHeight = null);
                document.querySelectorAll('.accordion-button .arrow').forEach(a => a.innerHTML = '&#9660;');
                panel.style.maxHeight = panel.scrollHeight + "px";
                arrow.innerHTML = '&#9650;';
            }
        });
    });

    const items = document.querySelectorAll('.text-item-perks');
    const iconDisplay = document.querySelector('.icon-display-perks');

    function setIconAndText(item) {
        iconDisplay.innerHTML = '';
        const icon = document.createElement('lord-icon');
        icon.setAttribute('src', item.getAttribute('data-icon-perks'));
        icon.setAttribute('trigger', 'loop');
        icon.setAttribute('colors', 'primary:#FFFFFF,secondary:#FFFFFF');
        icon.setAttribute('style', 'width:250px;height:250px');
        icon.setAttribute('delay', '2000');
        const text = document.createElement('div');
        text.textContent = item.getAttribute('data-text-perks');
        text.style.color = '#fff';
        text.style.fontSize = '20px';
        text.style.textAlign = 'center';
        text.style.marginTop = '20px';
        iconDisplay.appendChild(icon);
        iconDisplay.appendChild(text);
        iconDisplay.style.visibility = 'visible';
    }

    if (items.length > 0) {
        setIconAndText(items[0]);
    }

    items.forEach(item => {
        item.addEventListener('mouseenter', () => setIconAndText(item));
        item.addEventListener('mouseleave', () => {
            if (item !== items[0]) {
                iconDisplay.innerHTML = '';
                iconDisplay.style.visibility = 'hidden';
            }
        });
    });
});






{% endjavascript %}






