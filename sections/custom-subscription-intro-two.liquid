{% comment %} //////////////////PERKS SECTION///////////////////// {% endcomment %}
.master-section-container-perks{
  display: flex;
    flex-direction: column;
    justify-content: space-between;
    min-height:100vh;
    width:100vw;
    border:20px solid var(--padding-sections);
}
.master-title-subtitle-button-container-perks{
  height:20vh;
  display:flex;
  flex-direction:row;
  justify-content:space-between;
  align-items:center;
  padding:20px;

}
.container-perks {
    display: flex;
    flex-direction: row; /* Ensures side-by-side layout */
    height: calc(100vh - 20vh - 40px);
    width: 100%; /* Full viewport width */
    justify-content: stretch; /* Ensures that each child expands as needed */
    align-items: stretch; /* Aligns children to stretch to fill the container height */
    background-color:black;

}
.text-list-perks {
    height:100%;
    width: 50%; /* Half of the container */
    display: flex;
    flex-direction: column;
    justify-content: center; /* Vertically center content in this column */
    align-items: flex-start; /* Align items to the start (left side) of the column */
    border-right: 20px solid var(--padding-sections); /* Adds a border between the columns */
    box-sizing: border-box; /* Includes padding and border in the element's width */

}
.icon-display-perks {
    width: 50%; /* Half of the container */
    display: flex;
    align-items: center; /* Centers content vertically */
    justify-content: center; /* Centers content horizontally */
    font-size: 48px;
    visibility: visible; /* Always visible */
    box-sizing: border-box; /* Includes padding and border in the element's width */
    padding: 20px; /* Optional padding for aesthetics */

}
.text-item-perks{
    display: flex;
    flex-direction: row;
    align-items: center;
    background-color: black;
    height: calc(80vh / 8);
    width: 100%;
    padding-left:20px;
    border-bottom:4px solid var(--padding-sections);

}
.text-item-perks:hover {
  background-color:var(--padding-sections); 
}
.icon-display-perks {
    width: 50%;
    display: flex;
    flex-direction: column; // To stack icon and text vertically
    align-items: center; // Center-align the content horizontally
    justify-content: center; // Center-align the content vertically
    visibility: hidden; // Initially hidden, shown on hover
    transition: visibility 0.3s ease-in-out; // Smooth visibility transition
}


@media (max-width: 1100px) {
    .container-perks {
        flex-direction: column; /* Stacks the containers vertically on smaller screens */
    }
    .text-list-perks, .icon-display-perks {
        width: 100%; /* Full width of the container */
        border-right: none; /* Removes the border-right in stacked layout */
        border-bottom: 2px solid #000; /* Adds a border below the text list */
    }
    .icon-display-perks {
        display: none; /* Optionally hide the icon display on smaller screens */
    }
}

{% comment %} //////////////////ACCORDION FAQ SECTION///////////////////// {% endcomment %}

  .master-section-container-accordion{
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    min-height:100vh;
    width:100vw;
    border:20px solid var(--padding-sections);
  }
  .accordion-button {
      display: flex;
      justify-content: space-between;
      align-items: center;
      padding: 18px;
      width: 100%;
      background-color: black;
      color: white;
      font-size: 15px;
      border: none;
      outline: none;
      transition: 0.4s;
      cursor: pointer;
      border-bottom: 2px solid var(--padding-sections);
      text-align:left;
  }
  .button-text {
      flex-grow: 1; /* Ensures text takes up available space, pushing the arrow to the right */
  }
  .arrow {
      transition: transform 0.3s ease; /* Ensures smooth rotation */
      display: inline-block; /* Helps manage transformations better */
  }
  .arrow.up {
      transform: rotate(-180deg); /* Ensures the arrow points up */
  }
  .accordion-button:hover, .accordion-button:active {
      background-color: var(--padding-sections);
  }
  .panel {
      padding: 0 18px;
      background-color: white;
      max-height: 0;
      overflow: hidden;
      transition: max-height 0.2s ease-out;
      color:black;
  }
  .panel p {
    padding:5rem;
  }
  @media (max-width: 1100px) {


