{% javascript %} 

  document.addEventListener('DOMContentLoaded', function() {
      const tabButtons = document.querySelectorAll('.tab-btn');
      const tabContents = document.querySelectorAll('.content');
    
      tabButtons.forEach(function(button) {
        button.addEventListener('click', function() {
          const target = this.getAttribute('data-target');
    
          // Hide all tab contents
          tabContents.forEach(function(content) {
            content.style.display = 'none';
          });
    
          // Show the selected tab content
          document.getElementById(target).style.display = 'block';
    
          // Remove 'active' class from all buttons and add it only to the clicked button
          tabButtons.forEach(function(btn) {
            btn.classList.remove('active');
          });
          this.classList.add('active');
        });
      });
    });

    document.addEventListener('DOMContentLoaded', function() {
  function initializeSlider(containerClass) {
    const sliders = document.querySelectorAll(containerClass);

    sliders.forEach(function(slider) {
      let slideIndex = 0;
      const slides = slider.querySelectorAll('.slide');

      function showSlide(n) {
        slides.forEach(slide => slide.classList.remove('active'));
        slideIndex = (n + slides.length) % slides.length;
        slides[slideIndex].classList.add('active');
      }

      function nextSlide() {
        showSlide(slideIndex + 1);
      }

      function prevSlide() {
        showSlide(slideIndex - 1);
      }

      slider.querySelector('.next').addEventListener('click', nextSlide);
      slider.querySelector('.prev').addEventListener('click', prevSlide);
    });
  }

  // Initialize sliders for different containers
  initializeSlider('.section-SCHONHAUSER_ALLEE-padding.slider-container');
  initializeSlider('.section-MITTE-padding.slider-container');
  initializeSlider('.section-ROSENTHALER_PLATZ-padding.slider-container');
  initializeSlider('.section-NEUKOLLN-padding.slider-container');

  initializeSlider('.section-NORDBAHNHOF-padding.slider-container');
  initializeSlider('.section-HACKESCHER_MARKT-padding.slider-container');
  initializeSlider('.section-SONY_CENTER-padding.slider-container');
  initializeSlider('.section-POTSDAMER_PLATZ-padding.slider-container');
  initializeSlider('.section-CAFE_KRANZLER-padding.slider-container');
  initializeSlider('.section-ANNEX_CAFE-padding.slider-container');
  initializeSlider('.section-ROASTERY-padding.slider-container');
  initializeSlider('.section-MALLORCA-padding.slider-container');  
  initializeSlider('.section-SOUTH_KOREA-padding.slider-container');
  initializeSlider('.section-DUBAI-padding.slider-container');
});



