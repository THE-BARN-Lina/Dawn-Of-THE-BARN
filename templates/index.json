            "link": "https:\/\/thebarn.de\/products\/el-marcelo",
            "custom_liquid": "<style>\n.multicolumn-card__info a {\ntext-decoration: none;\n  text-decoration: inherit; \n}\n\n<\/style>"
          }
        },
        "1c72e7b1-7525-4448-9224-c7e5ed0db70b": {
