    "2178a1e0-fbe9-4b4f-8c5e-36afd31b043b": {
      "type": "image-with-text",
      "blocks": {
        "template--16379873853707__2178a1e0-fbe9-4b4f-8c5e-36afd31b043b-1669989047ec911251-0": {
